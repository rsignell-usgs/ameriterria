{
  "corsDomains" : [
      "data.amerigeoss.org",
      "canada.ca"
  ],
  "homeCamera": {
    "west": -121,
    "south": 18,
    "east": -66,
    "north": 81
  },
  "services": [],
  "catalog": [
    {
      "name": "Amerigeoss Canada",
      "type": "ckan",
      "url": "https://data.amerigeoss.org",
      "description": "Canada data",
      "minimumMaxScaleDenominator": 100,
      "filterByWmsGetCapabilities": false,
      "filterQuery": [
        {
          "fq": "+(organization:ca)+(res_format:(geojson OR GeoJSON OR kml OR KML OR kmz OR KMZ OR wms OR WMS OR \"Esri REST\"))"
        }
      ],
      "includeCsv": true,
      "includeGeoJson": true,
      "includeKml": true,
      "includeWms": true,
      "includeWfs": true,
      "includeEsriMapServer": true,
      "includeEsriFeatureServer": true
    }
  ]
}
