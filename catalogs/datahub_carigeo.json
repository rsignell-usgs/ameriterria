{
  "corsDomains" : [
      "data.amerigeoss.org",
      "icimod.org"
  ],
  "homeCamera": {
    "west": -121,
    "south": 18,
    "east": -66,
    "north": 61
  },
  "services": [],
  "catalog": [
    {
      "name": "Amerigeoss CARIGEO",
      "type": "ckan",
      "url": "https://data.amerigeoss.org",
      "description": "Caribbean Geospatial Development Initiative (CARIGEO)",
      "minimumMaxScaleDenominator": 100,
      "filterByWmsGetCapabilities": false,
      "filterQuery": [
        {
          "fq": "+(organization:carigeo)+(res_format:(geojson OR GeoJSON OR kml OR KML OR kmz OR KMZ OR wms OR WMS OR \"Esri REST\"))"
        }
      ],
      "includeCsv": true,
      "includeGeoJson": true,
      "includeKml": true,
      "includeWms": true,
      "includeWfs": true,
      "includeEsriMapServer": true,
      "includeEsriFeatureServer": true
    }
  ]
}
